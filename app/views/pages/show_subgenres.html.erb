<% if logged_in? %>
<table>                         
    <% @subgenres.each do |subgenre| %>       
    <tr>
        <td><%= subgenre.id %></td>  
        <td><%= link_to subgenre.name, quiz_questions_path(:subgenreId => subgenre.id, :i => 0), data: { confirm: 'Start the quiz' } %></td>  
    </tr>

    <% end %>
</table>     
<% else %>
<h1>Access Denied</h1>
<% end %>      