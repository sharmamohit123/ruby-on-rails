<% if logged_in? %>
<h1 class='title'>Leaderboard</h1>

    <% @subgenres.each do |subgenre| %>       
        <div class='link_box'><%= link_to subgenre.name, show_board_path(:subgenreId => subgenre.id, :genre => 0), class:'links' %></div>  

    <% end %>
<% else %>
<h1 class='title' >Access Denied</h1>
<% end %>  